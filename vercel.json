{
 
    "version" : 2,
    "builds" : [
        {
            "src" : "apii/wsgi",
            "use" :"@vercel/python",
            "config" : {"maxLambdaSize":"15mb" , "runtime": "python3.11"}
        },
        {
            "src" : "build_files.sh",
            "use" : "@vercel/static-build",
      
        }
    ],
    "routes" :[
        {
            "src" : "/static/(.*)",
            "dest" : "/static/$1"
        },
        {
            "src" :"/(.*)",
            "dest" :"apii/wsgi.py"
        }
    ]

} 